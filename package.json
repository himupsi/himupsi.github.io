{
  "name": "devlog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run removeOldDeloyFiles && vue-cli-service build --no-clean && npm run copy404",
    "lint": "vue-cli-service lint",
    "copy404": "cp ./docs/index.html ./docs/404.html",
    "updatePosts": "node ./helper/PostsDataMaker",
    "copyDemo": "cp -r ./demo/* ./docs/",
    "removeOldDeloyFiles": "rm -rf ./docs/css && rm -rf ./docs/js && rm ./docs/favicon.ico && rm ./docs/index.html && rm ./docs/404.html"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.20.1",
    "core-js": "^3.6.5",
    "highlight.js": "^10.4.1",
    "lodash": "^4.17.20",
    "marked": "^1.2.5",
    "moment": "^2.29.1",
    "vue": "^2.6.12",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "xml-js": "^1.6.11",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^5.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
